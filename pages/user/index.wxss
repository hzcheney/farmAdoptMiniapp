.wrapper {
  margin: 10px 10px;
  border-radius: 10rpx;
  font-size: 14px;
  background-color: white;
}
.settings {
  color: #323233;
  height: 250rpx;
  font-size: 14px;
  background: #11998e;
  background: linear-gradient(to top, #11998e 0%, #38ef7d 100%);
}
.cell-wrapper {
  width: 100%;
  display: table;
  padding: 40rpx 0;
}
.cell-left {
  width: 33.3%;
  height: 100%;
  text-align: left;
  display: table-cell;
  vertical-align: middle;
}
.scores {
  color: #323233;
  height: 100px;
  width: 94%;
  margin-left: 3%;
  height: 160rpx;
  background: #fff;
  border-radius: 10rpx;
  /* position: absolute; */
  margin-top: -50rpx;
  display: table;
}
.scores .center-cell {
  width: 25%;
  height: 160rpx;
  text-align: center;
  display: table-cell;
  vertical-align: middle;
}
.center-cell view {
  font-weight: 700;
}
.center-cell text {
  font-weight: 200;
}
.orders {
  width: 100%;
  height: 120rpx;
  display: table;
}
.order-cell {
  width: 25%;
  height: 100%;
  text-align: center;
  display: table-cell;
  vertical-align: middle;
}
.setting-cell {
  text-align: right;
  width: 25%;
  height: 100%;
  display: table-cell;
  vertical-align: middle;
}
.order-cell view {
  font-weight: 400;
}
.order-title {
  width: 96%;
  margin: auto;
  height: 60rpx;
  line-height: 60rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  justify-content: space-between;
  border-bottom: 1rpx solid #f4f4f4;
}
.order-title text {
  font-size: medium;
  font-weight: 400;
}
.common-func {
}
.more-tools {
}
.btn {
  display: table-cell;
  text-align: center;
  size: 14px;
  color: #323233;
}
